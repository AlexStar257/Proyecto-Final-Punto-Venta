<%- include('../partials/admin') %>
<table class="table table-dark table-bordered table-hover">
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Email</th>
        <th>Domicilio</th>
        <th>Telefono</th>
        <th>Tipo</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% if (data) { %>
        <% for(var i=0; i < data.length; i++) { %>
          <tr>
            <td><%= data[i].name %></td>
            <td><%= data[i].email %></td>
            <td><%= data[i].domicilio %></td>
            <td><%= data[i].telefono %></td>
            <td><%= data[i].tipo %></td>
            <td class="" style="white-space: nowrap;">
                <a href="/deleteUsuario/<%= data[i].email %>" class="btn btn-danger me-2">Eliminar</a>
              </td>
          </tr>
          <% } %>
            <% } %>
    </tbody>
  </table>